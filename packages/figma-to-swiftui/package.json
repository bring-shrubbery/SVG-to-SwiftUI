{
  "name": "figma-to-swiftui",
  "version": "0.0.0",
  "description": "Convert Figma Paths to SwiftUI",
  "scripts": {
    "build": "tsup src/index.ts --format iife --target es5",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix",
    "format": "prettier --write ./src --loglevel error"
  },
  "devDependencies": {
    "@quassum/prettier-config": "^1.2.0",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@figma/plugin-typings": "^1.77.0",
    "@types/hast": "^2.3.4",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/prettier": "^2.7.2",
    "@types/svg-parser": "^2.0.3",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "svg-to-swiftui-core": "workspace:*"
  },
  "license": "MIT",
  "author": {
    "name": "Antoni Silvestrovic",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "https://antoni.ai"
  }
}
