{
  "name": "figma-to-swiftui",
  "description": "Convert Figma Paths to SwiftUI",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format iife --target es5",
    "clean": "rimraf dist",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix",
    "format": "prettier --write ./src --loglevel error"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.96.0",
    "@svg-to-swiftui/prettier-config": "workspace:*",
    "@svg-to-swiftui/tsconfig": "workspace:*",
    "@swc/core": "^1.6.5",
    "@swc/jest": "^0.2.36",
    "@types/hast": "^3.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/svg-parser": "^2.0.6",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "svg-to-swiftui-core": "workspace:*"
  },
  "license": "MIT",
