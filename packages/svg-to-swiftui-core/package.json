{
  "name": "svg-to-swiftui-core",
  "version": "0.3.5",
  "description": "SVG to SwiftUI Shape converter core package. Will transform the raw SVG code into SwiftUI Shape via the JavaScript API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "svg",
    "swiftui",
    "transpiler",
    "swift",
    "converter"
  ],
  "author": {
    "name": "Antoni Silvestrovic",
    "email": "antoni.silvestrovic@gmail.com",
    "url": "https://antoni.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quassum/svg-to-swiftui-core.git"
  },
  "bugs": {
    "url": "https://github.com/quassum/svg-to-swiftui-core/issues"
  },
  "homepage": "https://github.com/quassum/svg-to-swiftui-core#readme",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify --external react",
    "clean": "rimraf dist",
    "test": "jest",
    "coverage": "jest --ci --coverage",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint ./src/**/* --fix",
    "format": "prettier --write ./src --loglevel error"
  },
  "devDependencies": {
    "@quassum/prettier-config": "^1.2.0",
    "@swc/core": "^1.3.24",
    "@swc/jest": "^0.2.24",
    "@types/hast": "^2.3.4",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/prettier": "^2.7.2",
    "@types/svg-parser": "^2.0.3",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.30.0",
    "gts": "^3.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "svg-parser": "^2.0.4",
    "svg-pathdata": "^6.0.3"
  }
}
